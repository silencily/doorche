<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="defaultCache">

    <diskStore path="../temp/doorche/ehcache"/>

    <!-- 默认缓存配置. -->
    <defaultCache maxEntriesLocalHeap="100" eternal="false" timeToIdleSeconds="300"
                  timeToLiveSeconds="600" overflowToDisk="true" maxEntriesLocalDisk="100000"/>

    <!-- 系统参数缓存 -->
    <cache name="systemParameterCache" maxEntriesLocalHeap="100" eternal="true" overflowToDisk="true"/>
    <!-- 系统字典缓存 -->
    <cache name="systemDictionaryCache" maxEntriesLocalHeap="100" eternal="true" overflowToDisk="true"/>

</ehcache>